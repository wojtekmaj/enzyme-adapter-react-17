{
  "name": "enzyme-test-suite",
  "private": true,
  "version": "1.0.0",
  "description": "JavaScript Testing utilities for React",
  "homepage": "https://enzymejs.github.io/enzyme/",
  "scripts": {
    "clean": "rimraf build",
    "lint": "eslint . --ext .jsx,.js",
    "mocha": "mocha --require @babel/register --extension js,jsx --recursive test",
    "prettier": "prettier --check . --cache",
    "test": "yarn lint && yarn prettier"
  },
  "keywords": [
    "javascript",
    "shallow rendering",
    "shallowRender",
    "test",
    "reactjs",
    "react",
    "flux",
    "testing",
    "test utils",
    "assertion helpers",
    "tdd",
    "mocha"
  ],
  "author": "Jordan Harband <ljharb@gmail.com>",
  "contributors": [
    "Wojciech Maj <kontakt@wojtekmaj.pl>"
  ],
  "license": "MIT",
  "dependencies": {
    "@wojtekmaj/enzyme-adapter-react-17": "portal:../",
    "@wojtekmaj/enzyme-adapter-utils": "^0.2.0",
    "chai": "^4.3.4",
    "create-react-class": "^15.7.0",
    "enzyme": "^3.11.0",
    "enzyme-shallow-equal": "^1.0.0",
    "html-element-map": "^1.3.0",
    "jsdom": "~15.1.0",
    "lodash.isequal": "^4.5.0",
    "mocha": "^10.1.0",
    "mocha-wrap": "^2.1.0",
    "object-inspect": "^1.11.0",
    "prop-types": "^15.7.0",
    "raf": "^3.4.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-is": "^17.0.0",
    "sinon": "^15.0.1",
    "sinon-sandbox": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.0",
    "@babel/core": "^7.15.0",
    "@babel/register": "^7.18.9",
    "eslint": "^8.26.0",
    "eslint-config-wojtekmaj": "^0.7.1",
    "prettier": "^2.7.0",
    "rimraf": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wojtekmaj/enzyme-adapter-react-17.git",
    "directory": "test"
  }
