    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-is": "^17.0.0",
    "safe-publish-latest": "^1.1.4",
    "semver": "^5.7.0",
    "sinon": "^5.1.1",
    "sinon-sandbox": "^2.0.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.0",
    "@babel/core": "^7.12.0",
    "eslint": "^7.15.0",
    "eslint-config-wojtekmaj": "^0.5.0",
    "rimraf": "^3.0.0"
  },
  "repository": {
    "type": "git",
