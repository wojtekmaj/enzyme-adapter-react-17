{
  "name": "enzyme-test-suite",
  "private": true,
  "version": "1.0.0",
  "description": "JavaScript Testing utilities for React",
  "homepage": "https://enzymejs.github.io/enzyme/",
  "scripts": {
    "build": "babel --source-maps=both test -d build",
    "clean": "rimraf build",
    "lint": "eslint . --ext .jsx,.js",
    "mocha": "yarn build && mocha --recursive build",
    "test": "yarn lint"
  },
  "keywords": [
    "javascript",
    "shallow rendering",
    "shallowRender",
    "test",
    "reactjs",
    "react",
    "flux",
    "testing",
    "test utils",
    "assertion helpers",
    "tdd",
    "mocha"
  ],
  "author": "Jordan Harband <ljharb@gmail.com>",
  "contributors": [
    "Wojciech Maj <kontakt@wojtekmaj.pl>"
  ],
  "license": "MIT",
  "dependencies": {
    "@wojtekmaj/enzyme-adapter-react-17": "portal:../",
    "chai": "^4.2.0",
    "create-react-class": "^15.6.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-helper": "^1.3.9",
    "enzyme-adapter-utils": "^1.14.0",
    "enzyme-shallow-equal": "^1.0.4",
    "has-symbols": "^1.0.1",
    "html-element-map": "^1.2.0",
    "jsdom": "^6.5.1",
    "lodash.isequal": "^4.5.0",
    "mocha": "^3.5.3",
    "mocha-wrap": "^2.1.2",
    "object-inspect": "^1.8.0",
    "object.assign": "^4.1.0",
    "prop-types": "^15.7.2",
